{
  "name": "prim",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "biome lint .",
    "format": "biome format .",
    "typecheck": "pnpm -r typecheck",
    "test": "pnpm -r test",
    "check": "pnpm -r lint && pnpm -r typecheck && pnpm -r test",
    "build:binaries": "pnpm --filter @primsh/keystore build:binaries",
    "gen:prims": "bun scripts/gen-prims.ts",
    "gen:openai": "bun scripts/gen-openai.ts",
    "gen:tests": "bun scripts/gen-tests.ts",
    "gen:check": "bun scripts/gen-prims.ts --check && bun scripts/gen-openai.ts --check && bun scripts/gen-tests.ts --check",
    "push:github-profile": "bun scripts/gen-prims.ts && bun scripts/push-github-profile.ts",
    "gate": "bun scripts/gate-check.ts",
    "deploy:prim": "bun scripts/deploy-prim.ts",
    "check:openapi": "bun scripts/check-openapi.ts",
    "sync:vps": "bash scripts/sync-vps.sh",
    "sync:vps:dry": "bash scripts/sync-vps.sh --dry-run",
    "test:conformance": "bun scripts/conformance.ts",
    "create-prim": "bun scripts/create-prim.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@coinbase/cdp-sdk": "^1.44.1",
    "@vitest/coverage-v8": "^1.6.1",
    "ajv": "^8.18.0",
    "ajv-formats": "^3.0.1",
    "bun-types": "^1.3.10",
    "typescript": "^5.6.3",
    "viem": "^2.21.0",
    "vitest": "^1.6.1"
  },
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "dependencies": {
    "aws4fetch": "^1.0.20",
    "yaml": "^2.8.2"
  }
}
