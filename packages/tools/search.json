[
  {
    "type": "function",
    "function": {
      "name": "search_web",
      "description": "Search the web and return ranked results with optional AI-generated answer. Price: $0.005 via x402 (USDC on Base).",
      "parameters": {
        "type": "object",
        "properties": {
          "query": {
            "type": "string",
            "description": "Search query string."
          },
          "max_results": {
            "type": "number",
            "description": "Maximum number of results to return. 1-20, default 10."
          },
          "search_depth": {
            "type": "string",
            "description": "Search depth. \"basic\" | \"advanced\", default \"basic\".",
            "enum": [
              "basic",
              "advanced"
            ]
          },
          "country": {
            "type": "string",
            "description": "Two-letter ISO 3166-1 country code to bias results (e.g. \"US\")."
          },
          "time_range": {
            "type": "string",
            "description": "Restrict results by recency. \"day\" | \"week\" | \"month\" | \"year\".",
            "enum": [
              "day",
              "week",
              "month",
              "year"
            ]
          },
          "include_answer": {
            "type": "boolean",
            "description": "Include AI-generated answer summarizing top results. Default false."
          },
          "include_domains": {
            "type": "array",
            "description": "Restrict results to these domains only (e.g. [\"docs.base.org\"]).",
            "items": {
              "type": "string"
            }
          },
          "exclude_domains": {
            "type": "array",
            "description": "Exclude results from these domains (e.g. [\"reddit.com\"]).",
            "items": {
              "type": "string"
            }
          }
        },
        "required": [
          "query"
        ]
      }
    }
  },
